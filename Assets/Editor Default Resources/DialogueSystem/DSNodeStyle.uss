
.ds-node__extension-container {
	background-color: #1d1d1d;
	padding: 8px;
}

.ds-node__custom-data-container {
	background-color: #1d1d1d;
	padding: 8px;
	border-radius: 4px;
}


.ds-node__textfield > .unity-text-field__input
{
	background-color: #1d1d1d;
	border-color: #ffffff;
	border-radius: 4px;
	max-width: 240px;
	min-width: 160px;
	padding: 8px;
}

.ds-node__textfield__hidden > .unity-text-field__input
{
	background-color: transparent;
	border-color: transparent;
}